{
  "name": "ngx-quill-upload",
  "version": "1.0.1",
  "description": "A module for images and videos to be uploaded to a server instead of being base64 encoded, in ngx-quill from toolbar editor.",
  "scripts": {
    "build": "ng-packagr -p package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayush013/ngx-quill-upload.git"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/package.schema.json",
    "lib": {
      "entryFile": "public_api.ts"
    },
    "dest": "dist"
  },
  "keywords": [
    "quill",
    "ngx-quill",
    "imageHandler",
    "videoHandler"
  ],
  "author": "Ayush Singh <ayush.singh.xda@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayush013/ngx-quill-upload/issues"
  },
  "dependencies": {
    "quill": "^1.3.7"
  },
  "homepage": "https://github.com/ayush013/ngx-quill-upload#readme",
  "devDependencies": {
    "@angular/cli": "^10.1.2",
    "@angular/common": "^10.0.11",
    "@angular/compiler": "^10.0.11",
    "@angular/compiler-cli": "^10.0.11",
    "@angular/core": "^10.0.11",
    "ng-packagr": "^10.1.0",
    "typescript": "^3.4.0"
  }
}
